{
  "extends": ["tslint:recommended", "tslint-config-prettier"],
  "linterOptions": {
    "exclude": ["**/*.js"]
  },
  "rules": {
    "object-literal-sort-keys": [false],
    "enum-member-name": [true, "caps-case"],
    "enum-name": [true, "caps-case"],
    "no-default-export": true,
    "interface-over-type-literal": false
  }
}
